<Home>:
    name: "Screen7"
    on_enter: 
        private_chats.show_storys_and_chats()
    canvas:
        Color:
            rgba: 1, 0.4, 0.2, 1
        Rectangle:
            size: self.size
            pos: self.pos
    NavBar:
        id: NavBar
        canvas.after:
            Color:
                rgba: (238/255, 250/255, 248/255, 1)
            Line:
                width: 1
                rounded_rectangle: self.x, self.y, self.width, 80, 0, 100
        PrivateChats:
            id: private_chats
        GroupChats:
        Announcements:
        SettingsNav: